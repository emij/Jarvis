#JSGF V1.0;

/**
 * JSGF Grammar for Voice controlled home, Jarvis
 *
 * @author Marika Hansson
 * @author Markus Otterberg
 */

grammar jarvis;

public <lamps> = Lamp | Lights | Room;
public <action> = Activate | Turn On | Turn Off;

public <light> = (<action>) (<lamps>);
public <room> = (Activate | Good morning) [<lamps>];